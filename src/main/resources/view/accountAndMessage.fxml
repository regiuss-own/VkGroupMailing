    <Button styleClass="white" text="Аккаунт: Илья Сбодунов" onAction="#onSelectAccountClick">
        <graphic>
            <Icon type="REGULAR" value="USER" size="15" />
        </graphic>
        <font>
            <Font size="16.0"/>
        </font>
    </Button>
    <Button styleClass="white" text="Набор сообщений: Приложение с картинкой" onAction="#onSelectMessageClick">
        <font>
            <Font size="16.0"/>
        </font>
    </Button>
</fx:root>
